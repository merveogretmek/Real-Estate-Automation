   "metadata": {},
   "outputs": [],
   "source": [
    "def click_button(xpath):\n",
    "    WebDriverWait(driver, wait_time).until(\n",
    "        EC.element_to_be_clickable((By.XPATH, xpath))\n",
    "    ).click()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "a8ed9df8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def click_checkbox(xpath, condition):\n",
    "    checkbox = WebDriverWait(driver, wait_time).until(\n",
    "        EC.element_to_be_clickable((By.XPATH, xpath))\n",
    "    )\n",
    "    if condition == 'Yes':\n",
    "        checkbox.click()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "ef540a38",
   "metadata": {},
   "outputs": [],
   "source": [
    "def handle_exceptions(func, *args):\n",
    "    try:\n",
    "        func(*args)\n",
    "    except ElementClickInterceptedException:\n",
    "        click_button('/html/body/div[7]/div[1]/div/div[2]/ul/li/a')\n",
    "        func(*args)\n",
    "    except NoSuchElementException:\n",
    "        time.sleep(5)\n",
    "        func(*args)\n",
    "    except StaleElementReferenceException:\n",
    "        time.sleep(5)\n",
    "        func(*args)"
   ]
  },
