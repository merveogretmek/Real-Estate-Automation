    "def wait_for_section_title(title):\n",
    "    title_locator = (By.XPATH, '//*[@id=\"container\"]/div[2]/div[1]/div/div/h1')\n",
    "    WebDriverWait(driver, wait_time).until(\n",
    "        EC.text_to_be_present_in_element(title_locator, title)\n",
    "    )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "d2032c6f",
   "metadata": {},
   "outputs": [],
   "source": [
    "def navigate_to_section(xpath):\n",
    "    WebDriverWait(driver, wait_time).until(EC.presence_of_element_located((By.XPATH, xpath)))\n",
    "    WebDriverWait(driver, wait_time).until(EC.element_to_be_clickable((By.XPATH, xpath)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "10c1a9b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def select_dropdown_text(xpath, text):\n",
    "    select = Select(driver.find_element(By.XPATH, xpath))\n",
    "    select.select_by_visible_text(text)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "3936b36b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def select_dropdown_value(xpath, value):\n",
    "    select = Select(driver.find_element(By.XPATH, xpath))\n",
    "    select.select_by_value(value)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "fee0fe60",
   "metadata": {},
   "outputs": [],
   "source": [
    "def enter_text(xpath, text):\n",
    "    field = driver.find_element(By.XPATH, xpath)\n",
    "    field.clear()\n",
    "    field.send_keys(text)"
   ]
  },
  {
   "cell_type": "code",
